local Config = {
    KeyURL = "https://example.com/key"
}

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = game.CoreGui

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 400, 0, 250)
Frame.AnchorPoint = Vector2.new(0.5, 0.5)
Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderColor3 = Color3.fromRGB(128, 0, 128)
Frame.BorderSizePixel = 3
Frame.Parent = ScreenGui

local FrameCorner = Instance.new("UICorner")
FrameCorner.CornerRadius = UDim.new(0, 15)
FrameCorner.Parent = Frame

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 50)
Title.Position = UDim2.new(0, 0, 0, 0)
Title.BackgroundTransparency = 1
Title.Text = "NexHub | Key System"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 24
Title.Parent = Frame

local TextBox = Instance.new("TextBox")
TextBox.Size = UDim2.new(0, 300, 0, 50)
TextBox.Position = UDim2.new(0.5, -150, 0.4, -25)
TextBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
TextBox.Text = ""
TextBox.Font = Enum.Font.SourceSansBold
TextBox.TextSize = 24
TextBox.PlaceholderText = "Enter Key"
TextBox.Parent = Frame

local TextBoxCorner = Instance.new("UICorner")
TextBoxCorner.CornerRadius = UDim.new(0, 10)
TextBoxCorner.Parent = TextBox

local GetKeyButton = Instance.new("TextButton")
GetKeyButton.Size = UDim2.new(0, 140, 0, 50)
GetKeyButton.Position = UDim2.new(0.25, -70, 0.75, -25)
GetKeyButton.BackgroundColor3 = Color3.fromRGB(128, 0, 128)
GetKeyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
GetKeyButton.Font = Enum.Font.SourceSansBold
GetKeyButton.TextSize = 22
GetKeyButton.Text = "Get Key"
GetKeyButton.Parent = Frame

local GetKeyCorner = Instance.new("UICorner")
GetKeyCorner.CornerRadius = UDim.new(0, 10)
GetKeyCorner.Parent = GetKeyButton

local SubmitButton = Instance.new("TextButton")
SubmitButton.Size = UDim2.new(0, 140, 0, 50)
SubmitButton.Position = UDim2.new(0.75, -70, 0.75, -25)
SubmitButton.BackgroundColor3 = Color3.fromRGB(128, 0, 128)
SubmitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
SubmitButton.Font = Enum.Font.SourceSansBold
SubmitButton.TextSize = 22
SubmitButton.Text = "Submit"
SubmitButton.Parent = Frame

local SubmitCorner = Instance.new("UICorner")
SubmitCorner.CornerRadius = UDim.new(0, 10)
SubmitCorner.Parent = SubmitButton

GetKeyButton.MouseButton1Click:Connect(function()
    setclipboard(Config.KeyURL)
    GetKeyButton.Text = "Copied!"
    wait(1)
    GetKeyButton.Text = "Get Key"
end)

SubmitButton.MouseButton1Click:Connect(function()
    local input = TextBox.Text
    if input == "" then
        SubmitButton.Text = "Empty!"
    else
        SubmitButton.Text = "Submitted"
        wait(1)
        SubmitButton.Text = "Submit"
    end
end)
